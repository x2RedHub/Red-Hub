--- Stats ---

local StatsSection = General:CreateSection({
    Name = "Stats",
    Side = "Right"
})
StatsSection:AddToggle({
    Name = "Melee",
	Value = _G.Melee,
    Callback = function(value)
        _G.melee= value
		_G.Settings.Auto_Stats_Melee = value
        saveSettings()
    end
})
StatsSection:AddToggle({
    Name = "Defense",
	Value = _G.Defense,
    Callback = function(value)
        _G.defense = value
		_G.Settings.Auto_Stats_Melee = value
        saveSettings()
    end
})
StatsSection:AddToggle({
    Name = "Sword",
	Value = _G.sword,
    Callback = function(value)
        _G.Sword = value
		_G.Settings.Auto_Stats_Melee = value
        saveSettings()
    end
})
StatsSection:AddToggle({
    Name = "Gun",
	Value = _G.gun,
    Callback = function(value)
        _G.Gun = value
		_G.Settings.Auto_Stats_Melee = value
        saveSettings()
    end
})
StatsSection:AddToggle({
    Name = "Demon Fruit",
	Value = _G.demonfruit,
    Callback = function(value)
        _G.Demonfruit = value
		_G.Settings.Auto_Stats_Melee = value
        saveSettings()
    end
})
StatsSection:AddSlider({
	Name = "Select Point",
	Value = 1,
	Min = 1,
	Max = 100,
	Format = "Point : %s%%",
	Callback = function(value)
		PointStats = value
	end
})
spawn(function()
	while wait() do
		if game.Players.localPlayer.Data.Points.Value >= PointStats then
			if _G.Melee then
				local args = {
					[1] = "AddPoint",
					[2] = "Melee",
					[3] = PointStats
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			end 
			if _G.Defense then
				local args = {
					[1] = "AddPoint",
					[2] = "Defense",
					[3] = PointStats
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			end 
			if _G.Sword then
				local args = {
					[1] = "AddPoint",
					[2] = "Sword",
					[3] = PointStats
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			end 
			if _G.Gun then
				local args = {
					[1] = "AddPoint",
					[2] = "Gun",
					[3] = PointStats
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			end 
			if _G.Demonfruit then
				local args = {
					[1] = "AddPoint",
					[2] = "Demon Fruit",
					[3] = PointStats
				}
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			end
		end
	end
end)
